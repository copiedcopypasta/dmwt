{
  "arrowParens": "always",
  "bracketSpacing": true,
  "endOfLine": "lf",
  "jsonRecursiveSort": true,
  "jsxSingleQuote": true,
  "plugins": [
    "prettier-plugin-tailwindcss",
    "prettier-plugin-sort-json",
    "prettier-plugin-packagejson"
  ],
  "printWidth": 80,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "all"
}
