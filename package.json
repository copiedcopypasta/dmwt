{
  "name": "dmwt",
  "version": "0.0.1",
  "private": true,
  "description": "A modern DMWT website for green-it.",
  "homepage": "https://site.ccp.2hoch1.dev",
  "bugs": {
    "url": "https://github.com/copiedcopypasta/dmwt/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/copiedcopypasta/dmwt.git"
  },
  "license": "MIT",
  "author": "CopiedCopyPasta (https://github.com/copiedcopypasta)",
  "contributors": [
    "copiedcopypasta/owners (https://github.com/copiedcopypasta/owners)"
  ],
  "type": "module",
  "scripts": {
    "prebuild": "npm run typecheck && npm run generator",
    "build": "next build",
    "commitlint": "commitlint",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate",
    "db:push": "prisma db push",
    "db:reset": "prisma migrate reset --force",
    "dev": "npm run dev:prepare && next dev",
    "dev:prepare": "npm run db:generate && npm run generator",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "generate:components": "npx -y tsx ./scripts/Components-Exporter.ts",
    "generate:routes": "npx -y tsx ./scripts/Routes-Exporter.ts",
    "generate:theme": "npx -y tsx ./scripts/Theme-Exporter.ts",
    "generator": "npm-run-all --parallel generate:*",
    "postinstall": "npm run db:generate",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "is-ci || husky install",
    "studio": "prisma studio",
    "tsc-check": "npm run typecheck",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@prisma/client": "^7.0.1",
    "@prisma/extension-accelerate": "^3.0.0",
    "@radix-ui/react-select": "^2.2.6",
    "@react-spring/parallax": "^10.0.3",
    "@svgr/webpack": "^8.1.0",
    "axios": "^1.13.2",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.555.0",
    "motion": "^12.23.25",
    "next": "^16.0.1",
    "next-seo": "^7.0.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-scroll-parallax": "^3.5.0",
    "simple-icons": "^16.1.0",
    "simple-parallax-js": "^6.3.3",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/eslintrc": "^3.3.1",
    "@next/eslint-plugin-next": "^16.0.5",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^8.0.3",
    "is-ci": "^4.1.0",
    "lint-staged": "^16.2.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "prettier": "3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-packagejson": "^2.5.20",
    "prettier-plugin-sort-json": "^4.1.1",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "prisma": "^7.0.1",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.20.6",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0"
  }
}
