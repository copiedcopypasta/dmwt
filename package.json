{
  "name": "dmwt",
  "version": "0.0.1",
  "private": true,
  "description": "A modern DMWT website for green-it.",
  "homepage": "https://site.ccp.2hoch1.dev",
  "bugs": {
    "url": "https://github.com/copiedcopypasta/dmwt/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/copiedcopypasta/dmwt.git"
  },
  "license": "MIT",
  "author": "CCP",
  "contributors": [
    "copiedcopypasta/owners (https://github.com/copiedcopypasta/owners)"
  ],
  "type": "module",
  "scripts": {
    "build": "next build",
    "ceg": "npm run generate:components",
    "commitlint": "commitlint",
    "dev": "npm run ceg && npm run lint && next dev",
    "generate:components": "npx -y ts-node ./scripts/CEG.ts",
    "postinstall": "prisma generate --no-engine",
    "lint": "eslint",
    "migrate": "npx -y ts-node ./scripts/migrate.ts",
    "prepare": "is-ci || husky install",
    "start": "next start",
    "tsc-check": "tsc --noEmit"
  },
  "dependencies": {
    "@prisma/client": "6.18.0",
    "@prisma/extension-accelerate": "2.0.2",
    "next": "16.0.1",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@eslint/eslintrc": "3",
    "@tailwindcss/postcss": "4",
    "@types/node": "24",
    "@types/react": "19",
    "@types/react-dom": "19",
    "eslint": "9",
    "eslint-config-next": "15.5.4",
    "husky": "8.0.3",
    "is-ci": "4.1.0",
    "lint-staged": "16.2.6",
    "prettier": "3.6.2",
    "prettier-plugin-packagejson": "2.5.19",
    "prettier-plugin-sort-json": "4.1.1",
    "prettier-plugin-tailwindcss": "0.6.14",
    "prisma": "6.18.0",
    "tailwindcss": "4",
    "tsx": "4.20.6",
    "typescript": "5.9.3"
  }
}
